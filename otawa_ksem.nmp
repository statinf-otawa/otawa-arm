		if setpre == 1 then add(t2, rn.i, t1); else _set(t2, rn.i); endif;
		load(rd.i, t2, UINT16);
		if setpre == 0 then add(rn.i, rn.i, t1);
		else if setw == 1 then _set(rn.i, t2);
