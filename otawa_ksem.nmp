extend ADD_shr
	otawa_ksem = {
		if shifter_operand.use_pc || rn.use_pc then seti(R(15), __IADDR + 8); endif;
		shifter_operand.otawa_sem;
		add(rd.i, rn.i, t1);
		if sets then seti(t1, 0); cmp(_CPSR, rd.i, t1); endif;
	}

