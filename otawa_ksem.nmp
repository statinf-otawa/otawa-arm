extend LDR_imm3_thumb
	otawa_ksem = { seti(t1, exts(imm) * 4); add(t1, R(13), t1); load(rd.i, t1, UINT32); }

extend LDRB_imm_thumb
	otawa_ksem = { seti(t1, imm); add(t1, rn.i, t1); load(rd.i, t1, UINT8); }

extend LDRB_shr_thumb
