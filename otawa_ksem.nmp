extend POP_thumb
	otawa_ksem = {
		ii = 0;
		seti(t2, 4);
		otawa_ksem_rec;
		if P == 1 then
			load(t1, R(13), UINT32);
			add(R(13), R(13), t2);
			_branch(t1);
		endif;
	}
	otawa_ksem_rec = {
		if ii < 8 then
			if llist<ii..ii> == 1 then
				load(R(ii), R(13), UINT32);
				add(R(13), R(13), t2);
			endif;
			ii = ii + 1;
			otawa_ksem_rec;
		endif;
	}

extend PUSH_thumb
	otawa_ksem = {
		seti(t2, 4);
		if P == 1 then
			sub(R(13), R(13), t2);
			store(t1, R(13), 4);
		endif;
		ii = 8;
		otawa_ksem_rec;
	}
	otawa_ksem_rec = {
		if ii > 0 then
			ii = ii - 1;
			if llist<ii..ii> == 1 then
				sub(R(13), R(13), t2);
				store(R(ii), R(13), 4);
			endif;
			otawa_ksem_rec;
		endif;
	}


